{
  "kind": "collectionType",
  "collectionName": "specialties",
  "info": {
    "singularName": "specialt",
    "pluralName": "specialties",
    "displayName": "Specialties ",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true,
      "regex": "const regex = /^(?![\\s-]+$).+/;"
    },
    "isActive": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "task": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::task.task",
      "mappedBy": "specialty"
    }
  }
}
